{
    "name": "rodrigoiii/page-blocker",
    "description": "This library helps you to block the page and give it time how long it will available to access again.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodrigo Galura III",
            "email": "rodrigogalura3rd@gmail.com"
        }
    ],
    "require": {
        "php": "^5.6.30 || ^7.0 || ^7.1"
    },
    "autoload": {
        "psr-4": {
            "PageBlocker\\": "src/"
        },
        "files": ["src/helpers.php"]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "platform": {
            "php": "5.6.30"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    }
}
